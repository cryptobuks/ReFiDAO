{
  "extends": ["eslint:recommended"],
  "env": {
    "browser": true,
    "amd": true,
    "es6": true
  },
  "plugins": [
    "babel"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "rules": {
    "no-console": 0,
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "no-undef": 0,
    "no-case-declarations": 0
  }
}
